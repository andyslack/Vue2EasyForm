{
  "version": "0.0.5",
  "name": "@me_slack/vueeasyform",
  "description": "A NPM for VueJS which makes working with forms really easy. Pass in a JSON object and get back a validated JSON object with the form field values.",
  "main": "dist/vueeasyform.umd.js",
  "module": "dist/vueeasyform.esm.js",
  "unpkg": "dist/vueeasyform.min.js",
  "browser": {
    "./sfc": "src/vueeasyform.vue"
  },
  "scripts": {
    "build": "npm run build:umd && npm run build:es && npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vueeasyform.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vueeasyform.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vueeasyform.min.js"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@vue/server-renderer": "^3.0.5",
    "html-webpack-plugin": "^4.5.0",
    "rollup": "^2.35.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^6.0.0",
    "ts-loader": "^8.0.13",
    "typescript": "^4.1.3",
    "vite": "^1.0.0-rc.13",
    "vue-loader": "^16.1.2",
    "webpack": "^5.11.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.1"
  },
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "peerDependencies": {
    "vue": "^3.0.4"
  },
  "dependencies": {
    "validatorjs": "^3.22.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyslack/VueEasyForm.git"
  },
  "keywords": [
    "form",
    "vue",
    "vuejs",
    "vueeasyform",
    "vueform",
    "vue-form",
    "vue-easy",
    "vue-easy-form"
  ],
  "author": "Andy Slack",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andyslack/VueEasyForm/issues"
  },
  "homepage": "https://github.com/andyslack/VueEasyForm#readme"
}
